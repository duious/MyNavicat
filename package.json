{
  "name": "MyNavicat",
  "version": "0.0.1",
  "description": "",
  "author": "duious",
  "private": true,
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint src --fix",
    "lint:create": "eslint --init",
    "dev": "vite",
    "build": "vite build",
    "electron": "cross-env NODE_ENV=dev node_modules/.bin/electron electron/main.js",
    "asar": "node_modules/.bin/asar pack dist dist/app.asar --unpack-dir \"{app.asar,build-apps,app.asar.unpacked}\"",
    "pack:prepare": "npm run build && cp -r electron/* dist/ && cp -r setting.json dist/",
    "pack:win": "node_modules/.bin/electron-builder --project=dist -w -p never",
    "pack:mac": "node_modules/.bin/electron-builder --project=dist -m -p never",
    "pack:mac:publish": "node_modules/.bin/electron-builder --project=dist -m -p always",
    "et": "node_modules/.bin/et -m",
    "sign": "cd electron/mas; bash sign.sh"
  },
  "dependencies": {
    "monaco-editor": "^0.21.2",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-beta.11"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/devtools": "^5.3.3",
    "cross-env": "^5.2.0",
    "electron": "^10.0.0",
    "electron-builder": "^22.8.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-store": "^6.0.0",
    "electron-updater": "^4.3.4",
    "eslint": "^7.10.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^6.2.2",
    "mitt": "^2.1.0",
    "mysql": "^2.18.1",
    "node-mysql": "^0.4.2",
    "sass": "1.26.10",
    "vite": "^1.0.0-rc.4"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  }
}
